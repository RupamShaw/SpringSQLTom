<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:context="http://www.springframework.org/schema/context"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="      
        http://www.springframework.org/schema/beans 
        http://www.springframework.org/schema/beans/spring-beans-3.2.xsd         
        http://www.springframework.org/schema/context 
        http://www.springframework.org/schema/context/spring-context-3.2.xsd">

	<context:property-placeholder
		location="classpath*:META-INF/properties/*.properties" />
	<bean id="studentController" class="org.app.controller.StudentController">
		<property name="studentManager" ref ="studentManager"/>
			
		
		
	</bean>
	<bean id="studentManager" name="studentManager" class="org.app.service.StudentMgr" />
	<context:component-scan base-package="org.app" />

</beans>
